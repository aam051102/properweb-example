{
	"compilerOptions": {
		"baseUrl": ".",
		"rootDir": ".",
		"outDir": "./dist",
		"jsx": "preserve",
		"jsxFactory": "JSX.createElement",
		"jsxFragmentFactory": "JSX.fragment",
		"lib": [
			"DOM",
			"DOM.Iterable",
			"ESNext",
		],
		"noImplicitAny": false
	},
	"include": [
		"./src/**/*.js",
		"./src/**/*.jsx",
	],
	"exclude": [
		"node_modules",
		"./dist"
	]
}